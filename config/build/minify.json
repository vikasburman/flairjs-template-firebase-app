{
    "js": {
        "warnings": "verbose",
        "mangle": true,
        "output": {
            "beautify": false,
            "comments": "some"
        },
        "compress": {
            "dead_code": false,
            "evaluate": false,
            "unused": false,
            "warnings": true,
            "passes": 1,
            "global_defs": {
                "global": true,
                "window": true        
            }
        }
    },

    "css": {
        "level": 2
    },

    "html": {
        "collapseBooleanAttributes": true,
        "collapseWhitespace": true,
        "conservativeCollapse": true,
        "decodeEntities": true,
        "removeComments": true,
        "minifyCSS": true,
        "minifyJS": true
    }
}
