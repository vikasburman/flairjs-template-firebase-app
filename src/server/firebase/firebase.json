{
  "database": {
    "rules": "database/database.rules.json"
  },
  "firestore": {
    "rules": "firestore/firestore.rules",
    "indexes": "firestore/firestore.indexes.json"
  },
  "functions": {
    "source": "functions"
  },
  "hosting": {
